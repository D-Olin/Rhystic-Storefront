<div class="container-fluid text-center">
    <div class="row">
        <div class="col" style="height: auto;">
            {{>user_profile}}
        </div>
        <div class="col" style="background-color:lightblue;">
            <table>
                <tr>
                    <th>Card Name</th>
                    <th>Card Price</th>
                </tr>
                {{#each card}}
                    <tr>
                        <td>{{>card_name}}</td>
                        <td>{{>card_price}}</td>
                    </tr>
                {{/each}}
            </table>
        </div>
    </div>
</div>
